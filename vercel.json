{
  "functions": {
    "pages/[pageId].tsx": {
      "maxDuration": 30
    },
    "pages/index.tsx": {
      "maxDuration": 30
    },
    "pages/api/cache-warmup.ts": {
      "maxDuration": 60
    },
    "pages/api/cache-clear.ts": {
      "maxDuration": 30
    },
    "pages/api/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=4096"
    }
  }
}